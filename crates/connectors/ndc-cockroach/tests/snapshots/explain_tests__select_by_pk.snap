EXPLAIN
SELECT
  row_to_json("universe") AS "universe"
FROM
  (
    SELECT
      *
    FROM
      (
        SELECT
          coalesce(json_agg(row_to_json("rows")), '[]') AS "rows"
        FROM
          (
            SELECT
              "Album"."Title" AS "Title"
            FROM
              "public"."Album" AS "Album"
            WHERE
              (
                true
                AND ("Album"."AlbumId" = 35)
              )
          ) AS "rows"
      ) AS "rows"
  ) AS "universe"
