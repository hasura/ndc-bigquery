SELECT
  JSON_OBJECT('rows', ARRAY_AGG(universe_2)) AS universe
FROM
  (
    SELECT
      *
    FROM
      (
        SELECT
          album_0.Title AS title
        FROM
          public.Album AS album_0
        WHERE
          EXISTS (
            SELECT
              1 AS one
            FROM
              public.Artist AS artist_1
            WHERE
              (
                (
                  true
                  AND (artist_1.Name = cast(@param1 as string))
                )
                AND (album_0.ArtistId = artist_1.ArtistId)
              )
          )
      ) AS rows_3
  ) AS universe_2

[(1, String("Gilberto Gil"))]
