SELECT
  row_to_json("universe") AS "universe"
FROM
  (
    SELECT
      *
    FROM
      (
        SELECT
          coalesce(json_agg(row_to_json("rows")), '[]') AS "rows"
        FROM
          (
            SELECT
              "%RELATIONSHIP_0_FOR_Artist"."Artist" AS "Artist"
            FROM
              "public"."Album" AS "Album"
              LEFT OUTER JOIN LATERAL (
                SELECT
                  row_to_json("%RELATIONSHIP_0_FOR_Artist") AS "Artist"
                FROM
                  (
                    SELECT
                      *
                    FROM
                      (
                        SELECT
                          coalesce(json_agg(row_to_json("rows")), '[]') AS "rows"
                        FROM
                          (
                            SELECT
                              "Artist"."Name" AS "Name"
                            FROM
                              "public"."Artist" AS "Artist"
                            WHERE
                              (
                                true
                                AND ("Album"."ArtistId" = "Artist"."ArtistId")
                              )
                          ) AS "rows"
                      ) AS "rows"
                  ) AS "%RELATIONSHIP_0_FOR_Artist"
              ) AS "%RELATIONSHIP_0_FOR_Artist" ON ('true')
            LIMIT
              5
          ) AS "rows"
      ) AS "rows"
  ) AS "universe"

